; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Godot Combat Test"
run/main_scene="res://world.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Constants="*res://scripts/singletons/constants.gd"
Signal_Debugger="*res://addons/SignalVisualizer/Debugger/SignalDebugger.gd"

[display]

window/size/viewport_width=720
window/size/viewport_height=480

[editor_plugins]

enabled=PackedStringArray("res://addons/SignalVisualizer/plugin.cfg")

[layer_names]

2d_physics/layer_1="team1_hurtbox"
2d_physics/layer_2="team1_collision"
2d_physics/layer_3="team2_hurtbox"
2d_physics/layer_4="team2_collision"

[physics]

2d/default_gravity=0.0
2d/default_gravity_vector=Vector2(0, 0)
